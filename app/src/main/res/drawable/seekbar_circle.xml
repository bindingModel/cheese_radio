<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item

      >
        <shape
            android:shape="oval">
            <!--<stroke
                android:width="10px"
                android:color="@color/text_white" />-->
            <gradient
                android:startColor=" #00000000"
                android:centerColor="#12111111"
                android:endColor=" #00000000"
                android:angle="180" />
            <corners android:radius="0dp"/>
            <size
                android:width="2.16dp"
                android:height="20.16dp" />
        </shape>
    </item>
    <item
        android:bottom="1.5dp"
        android:right="1dp"
        android:left="1dp"
       >
        <shape android:shape="oval"
            xmlns:android="http://schemas.android.com/apk/res/android">
          <!--  <stroke
                android:width="1px"
                android:color="@color/text_white" />-->
            <size
                android:width="20.16dp"
                android:height="20.16dp" />

            <solid android:color="#ffffff" />

        </shape>
    </item>





</layer-list>

