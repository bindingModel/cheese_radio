<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cheese.radio">
    <application android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.home.HomeActivity"/>
        <activity android:name=".ui.startup.StartUpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.anchors.AnchorsActivity"/>
        <activity android:name=".ui.login.LoginActivity"/>
        <activity android:name=".ui.guide.GuideActivity"/>
        <activity android:name=".ui.user.profile.ProfileActivity" android:label="个人信息"/>
    </application>
</manifest>
